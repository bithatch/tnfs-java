#!/bin/sh
# postinst script for [[project.id]]

set -e

# source debconf stuff
. /usr/share/debconf/confmodule

case "$1" in

    configure)
	    systemctl daemon-reload
		systemctl enable tnfsjd
		systemctl enable tnfsjd-web
		systemctl start tnfsjd
		systemctl start tnfsjd-web
    ;;
    
    triggered|abort-upgrade|abort-remove|abort-deconfigure)
    ;;

    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

exit 0
